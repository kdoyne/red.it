<%= link_to "Sign Up", new_user_path %>
<%= link_to "Submit Story", new_story_path %>

<% @stories.each do |story| %>
<%=  story.url %>


  <%= form_tag upvote_path(story) do %>
  <%= hidden_field_tag :story, story.id %>
  <%= submit_tag("upvote") %>
  <%end%>

  <%= form_tag downvote_path(story) do %>
  <%= hidden_field_tag :story, story.id %>
  <%= submit_tag("downvote") %>
  <%end%>

  
<% end %>

